{
  "name": "@tito10047/progressive-image-bundle",
  "description": "High-performance progressive image loading for Symfony. Deliver lightning-fast user experiences by serving beautiful Blurhash placeholders.",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "symfony-ux",
    "twig"
  ],
  "symfony": {
    "controllers": {
      "progressive-image": {
        "main": "controllers/progressive-image_controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "@tito10047/progressive-image-bundle/styles/style.css": true
        }
      }
    },
    "importmap": {
      "blurhash": "2.0.5"
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  }
}
